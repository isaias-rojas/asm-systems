
Lea bx, cadest
Lea si, cadfij
inc bx
mov cl, [bx]
mov dl, 4
inc bx
ciclo:
    inc bx
    loop ciclo
    
mov cl, dl
    
copiar:
    mov al, [si]
    mov [bx], al
    inc bx
    inc si

    loop copiar
continuar:  
    Lea bx, cadest
    inc bx
    mov cl, dl
actualizar:
    inc [bx]
    loop actualizar
salir:    
    ret




cadest db 100, 4, "asdf"
cadfij db "1234"
